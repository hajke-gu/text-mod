{
    "apiVersion": "1.0",
    "version": "1.0",
    "name": "Text Visualization",
    "id": "js-dev-starter",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        }
    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
        "axes": [
            {
                "name": "Content",
                "mode": "categorical"
            },
            {
                "name": "Annotation",
                "mode": "categorical"
            },
            {
                "name": "Sorting",
                "mode": "categorical"
            },
            {
                "name": "Tooltip",
                "mode": "categorical"
            }
        ]
    },
    "files": ["index.html", "main.css", "main.js"]
}
